<h1> New photo </h1>
<%= form_for(@photo) do |f| %>
  <%= render 'devise/shared/error_messages', resource: f.object %>
  <div class="field">
    <br>
  <span class="picture">
    <%= f.file_field :picture, accept: 'image/jpeg,image/gif,image/png' %>
  </span>
  </div>
  <br>
  <%= f.submit "Upload", :class => "btn btn-primary"  %>
<% end %>
